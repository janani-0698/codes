import java.util.*;

public class RemoveDuplicates {
    public static void main(String[] args) {
        int[] arr = {11, 43, 67, 45, 23, 11, 67, 45};

        
        Set<Integer> set = new LinkedHashSet<>();
        for (int num : arr) {
            set.add(num);
        }

    
        int[] result = new int[set.size()];
        int i = 0;
        for (int num : set) {
            result[i++] = num;
        }

    
        System.out.println(Arrays.toString(result));
    }
}
